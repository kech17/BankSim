import java.util.*;


public class BankAccount
{
	private int max;
	private int withdraw;
	private double total;
	private double commission;
	String username;
	String accType;

	public BankAccount(String name, String accType, int max, int withdrawl, double total)
	{
		if (max>0 && withdraw>0 && total>=0)
		{
			username = name;
			this.max = max;
			this.username = username;
			this.withdraw = withdraw;
			this.accType = accType;
			if (accType == "high interest")
				commission = 0.0302;
			else if (accType == "low interest")
				commission = 0.0105;
		}
		else
		{
			System.out.println("Invalid input");
		}
	}

	public int getMax() { return max; }
	public double getTotal() { return total; }
	public int getWithdraw() { return withdraw; }
	public int getName() { return username; }
	public int getCommission() { return commission; }

	public boolean Deposit(double deposit)
	{
		if (deposit + total > max)
		{
			System.out.println("Exceeds maximum account limit");
			return false;
		}
		else
		{
			total += deposit;
			System.out.println("Deposit succeeded");
			return true;
		}
	}

	public boolean Withdraw(int withdraw)
	{
		if (withdraw < 0)
		{
			System.out.println("Cannot withdraw negative amount");
			return false;
		}
		else if (withdraw > total)
		{
			System.out.println("You only have $" + total);
			return false;
		}
		else if (withdraw > this.withdraw)
		{
			System.out.println("Withdraw limit is $" + this.withdraw);
			return false;
		}
		else
		{
			total -= withdraw;
			System.out.println("Withdraw suceeded");
			return true;
		}
	}

	public String toString()
	{
		String data = String.Format("%s %10s %10s %20s", username, total, max, withdraw);

		return (data);
	}

	public boolean hasAssets()
	{
		return (getTotal() > 0);
	}
}
